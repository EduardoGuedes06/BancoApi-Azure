            </div>
            <div class="form-group">
                <label asp-for="Senha8dig" class="control-label"></label>
                <input asp-for="Senha8dig" class="form-control" />
                <span asp-validation-for="Senha8dig" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Senha6dig" class="control-label"></label>
                <input asp-for="Senha6dig" class="form-control" />
                <span asp-validation-for="Senha6dig" class="text-danger"></span>
